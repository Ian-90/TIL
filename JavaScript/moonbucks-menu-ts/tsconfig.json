{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost"],
    "module": "ES2015",
    "rootDir": "./src",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
